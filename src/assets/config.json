{
  "recordTypes": [
    "all"
  ],
  "all": {
    "solrUrl": "http://localhost:8980/",
    "mainQuery": "?q=<%= queryFields %>&rows=<%= rows %>&start=<%= start %><%= facetFields %>",
    "queryFieldsBoolOperator": " OR ",
    "queryFieldValPrefix": "",
    "queryFieldValSuffix": "",
    "queryFields": [
      "main_search"
    ],
    "fieldList": "&fl=*",
    "rows": 10,
    "paginationSize": 10,
    "groupSearchRefinersBy": "type_______facet_multi",
    "maxGroupedResultsCount": 5,
    "hideFiltersWhenSearching": true,
    "sortGroupHeadersBy": ["name"],
    "searchRefiners": [
      {
        "name": "type_______facet_multi",
        "title": "@value",
        "type": "facet",
        "typeLabel": null,
        "alwaysActive": true
      },
      {
        "name": "https___schema_org_keywords_______https___schema_org_Dataset_______facet_multi",
        "title": "search-refine-keywords",
        "type": "facet",
        "typeLabel": null,
        "alwaysActive": true,
        "targetRecordType": "https___schema_org_Dataset"
      },
      {
        "name": "https___schema_org_publisher_______https___schema_org_Dataset_______facet",
        "title": "search-refine-publisher",
        "type": "facet",
        "typeLabel": null,
        "alwaysActive": true,
        "targetRecordType": "https___schema_org_Dataset"
      },
      {
        "name": "https___schema_org_birthPlace_______https___schema_org_Person_______facet",
        "title": "search-refine-birthPlace",
        "type": "facet",
        "typeLabel": null,
        "alwaysActive": true,
        "targetRecordType": "https___schema_org_Person"
      },
      {
        "name": "https___schema_org_birthDate_______https___schema_org_Person_______facet",
        "title": "search-refine-birthDate",
        "type": "facet",
        "typeLabel": null,
        "alwaysActive": true,
        "targetRecordType": "https___schema_org_Person"
      }
    ]
  },
  "https___schema_org_Dataset": {
    "solrUrl": "http://localhost:8980/",
    "mainQuery": "?q=type_______facet_multi:<%= type %> AND (<%= queryFields %>)<%= fieldList %>,[child%20parentFilter=type:<%= type %>]&rows=<%= rows %>&start=<%= start %><%= facetFields %>",
    "queryFieldsBoolOperator": " OR ",
    "queryFieldValPrefix": "",
    "queryFieldValSuffix": "",
    "queryFields": [
      "main_search"
    ],
    "fieldList": "&fl=*",
    "rows": 10,
    "paginationSize": 10,
    "maxGroupedResultsCount": 5,
    "searchRefiners": [
      {
        "name": "https___schema_org_keywords_______https___schema_org_Dataset_______facet_multi",
        "title": "search-refine-keywords",
        "type": "facet",
        "typeLabel": null,
        "alwaysActive": true
      },
      {
        "name": "https___schema_org_publisher_______https___schema_org_Dataset_______facet",
        "title": "search-refine-publisher",
        "type": "facet",
        "typeLabel": null,
        "alwaysActive": true
      }
    ],
    "searchResultDisplay": [
      {"link": true, "type": "external", "field": "id", "template": "<%= data['https___schema_org_name'] %>", "class":"text-justify" },
      {"template": "<p></p>" },
      {"template": "<p><%= data['https___schema_org_description'] %></p>",  "class":"text-justify" },
      {"template": "<p></p>" },
      {"template": "<p>Published: <%= moment(data['https___schema_org_datePublished'][0]) %> <%= !_.isEmpty(utilService.getChildDocWithType(data, 'https___schema_org_license', null)) ? '| License: ' + utilService.getChildDocWithType(data, 'https___schema_org_license')['https___schema_org_name'][0] : '' %></p>",  "class":"text-justify" }
    ]
  },
  "https___schema_org_Person": {
    "solrUrl": "http://localhost:8980/",
    "mainQuery": "?q=type_______facet_multi:<%= type %> AND (<%= queryFields %>)<%= fieldList %>&rows=<%= rows %>&start=<%= start %><%= facetFields %>",
    "queryFieldsBoolOperator": " OR ",
    "queryFieldValPrefix": "",
    "queryFieldValSuffix": "",
    "queryFields": [
      "main_search"
    ],
    "fieldList": "&fl=*",
    "rows": 10,
    "paginationSize": 10,
    "maxGroupedResultsCount": 5,
    "searchRefiners": [
      {
        "name": "https___schema_org_birthPlace_______https___schema_org_Person_______facet",
        "title": "search-refine-birthPlace",
        "type": "facet",
        "typeLabel": null,
        "alwaysActive": true
      },
      {
        "name": "https___schema_org_birthDate_______https___schema_org_Person_______facet",
        "title": "search-refine-birthDate",
        "type": "facet",
        "typeLabel": null,
        "alwaysActive": true
      }
    ],
    "searchResultDisplay": [
      {"link": true, "type": "external", "field": "id", "template": "<%= data['https___schema_org_name'] %>", "class":"text-justify" },
      {"template": "<p></p>" },
      {"template": "<p><%= data['https___schema_org_description'] %></p>",  "class":"text-justify" }
    ]
  },
  "default": {
    "solrUrl": "http://localhost:8980/",
    "mainQuery": "?q=type_______facet_multi:<%= type %> AND (<%= queryFields %>)<%= fieldList %>,[child%20parentFilter=type:<%= type %>]&rows=<%= rows %>&start=<%= start %><%= facetFields %>",
    "queryFieldsBoolOperator": " OR ",
    "queryFieldValPrefix": "",
    "queryFieldValSuffix": "",
    "queryFields": [
      "main_search"
    ],
    "fieldList": "&fl=*",
    "rows": 10,
    "paginationSize": 10,
    "maxGroupedResultsCount": 5,
    "propertyDumpExceptionList": ["^type", "^_", "_str$", "_facet$","_facet_multi$", "^record_", "id", "https___schema_org_name", "https___schema_org_description", "main_search"],
    "propertyDumpRecurse": true,
    "propertyDumpRecurseExceptionList": ["^type", "^_", "_str$", "_facet$","_facet_multi$", "^record_", "main_search"],
    "searchResultDisplay": [
      {"template": "<p class='h4 text-white bg-secondary p-3'><%= data['https___schema_org_name'] %></p>", "class":"text-justify" },
      {"template": "<p><%= data['https___schema_org_description'] %></p>",  "class":"text-justify" },
      {"template": "<p class='h6'><%= translationService.t('id-label') %>: <%= utilService.transformToLink(data['id']) %></p>", "class":"text-justify" },
      {"template": "<p class='h6'><%= translationService.t('type-label') %>: <%= utilService.getArrAsLinks(data['type'], '_blank', '<ul>', '</ul>', '<li>', '</li>') %> </p>"},
      {"template": "<p></p>" },
      {"template": "<p><%= utilService.propertyDump(data, true, config.default.propertyDumpExceptionList, translationService, config.default.propertyDumpRecurse, config.default.propertyDumpRecurseExceptionList) %> </p>"}
    ]
  }
}
